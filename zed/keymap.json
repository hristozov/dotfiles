[
  {
    "use_key_equivalents": true,
    "bindings": {
      "cmd-shift-w": ["pane::CloseInactiveItems", { "close_pinned": false }]
    }
  },
  {
    "context": "ProjectPanel",
    "bindings": {
      "cmd-shift-w": ["pane::CloseInactiveItems", { "close_pinned": false }]
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd-shift-w": ["pane::CloseInactiveItems", { "close_pinned": false }]
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "cmd-[": "pane::GoBack",
      "cmd-]": "pane::GoForward",
      "cmd-d": "editor::DuplicateLineUp",
      "alt-up": "editor::SelectLargerSyntaxNode",
      "alt-down": "editor::SelectSmallerSyntaxNode",
      "alt-shift-up": "editor::MoveLineUp",
      "alt-shift-down": "editor::MoveLineDown",
      "cmd-shift-w": ["pane::CloseInactiveItems", { "close_pinned": false }],
      "alt-shift-f": "editor::Format"
    }
  },
  {
    "context": "Pane",
    "use_key_equivalents": true,
    "bindings": {
      "cmd-[": "pane::GoBack",
      "cmd-]": "pane::GoForward",
      "cmd-shift-w": ["pane::CloseInactiveItems", { "close_pinned": false }],
      "alt-\\": "pane::SplitRight"
    }
  }
]
